/* pages/othen/index.wxss */
@import "../template/template.wxss";
page{
  width: 100%;
  height: 100%;
}
.content{
  width: 100%;
  height: 100%;
  position: relative;
}
.bj{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  min-height:100%;
}
.text{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: auto;
}
#content{
  width:100%;
  height: 300px;
}
.font{
  color: #fff;
  text-align: center;
  width: 100%;
  height: 100px;
  font-size: 18px;
  line-height: 100px;
  display: block;
}
.banner{
  width: 100%;
  height: 150px;
  margin: 0 auto;
}
.swiper{
  width: 100%;
  height: 100%;
}
.bannerImg{
  width: 100%;
}
.root{
  width: 30%;
  height: 50px;
  position: fixed;
  bottom: 10px;
  left: 10px;
}
